    "chai": "^5.1.2",
    "chai-http": "^5.1.1",
    "mocha": "^10.7.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.13.0",
